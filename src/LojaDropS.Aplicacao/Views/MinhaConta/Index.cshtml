<h5>Dados</h5>
<div class="row">
    <div class="col">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Tipo</th>
                    <th>Valor</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var claim in User.Claims) {
                    <tr>
                        <td> @claim.Type </td>
                        <td> @claim.Value </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>